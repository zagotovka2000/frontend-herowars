{
   "version": 2,
   "builds": [
     {
       "src": "**/*.html",
       "use": "@vercel/static"
     }
   ],
   "routes": [
     {
       "src": "/css/(.*)",
       "dest": "/css/$1"
     },
     {
       "src": "/js/(.*)",
       "dest": "/js/$1"
     },
     {
       "src": "/(.*)",
       "dest": "/index.html"
     }
   ]
 }
